        "embedding_dims": 2560
      }
    },
    "vector_store": {
      "provider": "tencent_vectordb",
      "config": {
        "url": "http://gz-vdb-h9v0pcq0.sql.tencentcdb.com:8100",
        "key": "env:TENCENT_VECTORDB_KEY",
        "username": "root",
        "database_name": "openmemory",
        "collection_name": "openmemory_collection",
        "embedding_model_dims": 2560,
        "metric_type": "cosine",
        "timeout": 60
